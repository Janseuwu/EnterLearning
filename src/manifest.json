{
	"manifest_version": 3,
	"name": "EnterLearning",
	"version": "1.3",
	"description": "Adds various features on ItsLearning",
	"browser_specific_settings": {
		"gecko": { 
			"id": "EnterLearning@13471398948"
		}
	},
	"content_scripts": [
		{
			"matches": ["*://*.itslearning.com/*"],
			"js": ["js/content.js"]
		}
	],

	"permissions": [
		"declarativeNetRequest"
	],

	"declarative_net_request": {
		"rule_resources": [{
			"id": "ruleset",
			"enabled": true,
			"path": "rules.json"
		}]
	}
}
